{
  "name": "cwtail",
  "version": "1.4.1",
  "description": "CloudWatch Logs Tail",
  "main": "./cwtail.js",
  "bin": {
    "cwtail": "./cli.js"
  },
  "scripts": {
    "preinstall": "node -e 'if(/yarn\\.js$/.test(process.env.npm_execpath))throw new Error(\"This repo requires npm\")'",
    "lintcli": "./node_modules/.bin/eslint ./cli.js",
    "lintlib": "./node_modules/.bin/eslint ./cwtail.js",
    "lint": "npm run lintcli && npm run lintlib",
    "pretest": "npm run lint",
    "test": "echo \"Error: no test specified\" && exit 1",
    "coverage": "./node_modules/.bin/nyc npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kennu/cwtail.git"
  },
  "author": "Kenneth Falck <kennu@iki.fi>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kennu/cwtail/issues"
  },
  "homepage": "https://github.com/kennu/cwtail#readme",
  "dependencies": {
    "aws-sdk": "2.x",
    "yargs": "15.x"
  },
  "devDependencies": {
    "chai": "*",
    "eslint": "*",
    "mocha": "*",
    "nyc": "*"
  }
}
